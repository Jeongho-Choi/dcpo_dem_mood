	Keywords = {Opinion-Policy},
	Number = {1},
	Pages = {115-129},
	Title = {Estimating the Political Center from Aggregate Data: An Item Response Theory Alternative to the Stimson Dyad Ratios Algorithm},
	Volume = {22},
	Year = {2014},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://pan.oxfordjournals.org/content/22/1/115.abstract},
